@model Incomes.ViewModels.Incomes.IndexViewModel
<h1 class="master-detail-view__title">
  Incomes
</h1>
<table class="master-detail-view__table table" cellpadding="0" cellspacing="0">
  <tr class="table__row">
    <td class="table__cell table__cell--header">Amount</td>
    <td class="table__cell table__cell--header">Created</td>
  </tr>
  @foreach (var income in this.Model.Incomes)
  {
    @Html.Partial("_Income", income)
  }
</table>
<div class="buttons">
  <a class="buttons__button button" href="/incomes/create">Create income</a>
</div>
